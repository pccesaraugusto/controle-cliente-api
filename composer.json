{
  "name": "pccesaraugusto/clientes-service",
  "description": "Microservice de controle de clientes - Laravel API scaffold",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "darkaonline/l5-swagger": "^9.0"
  },
  "require-dev": {
    "pestphp/pest": "^3.0",
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
